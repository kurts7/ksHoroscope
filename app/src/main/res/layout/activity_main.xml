<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/main_window">

<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:id="@+id/first_line">
    <ImageButton
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:id="@+id/aries"
        android:src="@drawable/aries_selector"
        android:clickable="true"
        android:baselineAlignBottom="false"
        android:scaleType="fitCenter"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        android:layout_alignTop="@+id/imageButtonGemini"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@null" />
    <ImageButton
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:id="@+id/taurus"
        android:src="@drawable/taurus_selector"
        android:clickable="true"
        android:baselineAlignBottom="false"
        android:scaleType="fitCenter"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        android:layout_alignTop="@+id/imageButtonAries"
        android:layout_toRightOf="@+id/imageButtonAries"
        android:layout_toEndOf="@+id/imageButtonAries"
        android:background="@null"/>
    <ImageButton
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:id="@+id/gemini"
        android:src="@drawable/gemini_selector"
        android:clickable="true"
        android:baselineAlignBottom="false"
        android:scaleType="fitCenter"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButtonTaurus"
        android:layout_toEndOf="@+id/imageButtonTaurus"
        android:background="@null"/>
</LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:id="@+id/second_line"
        android:layout_below="@+id/first_line"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">
        <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/cancer"
            android:src="@drawable/cancer_selector"
            android:clickable="true"
            android:baselineAlignBottom="false"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:background="@null"/>
        <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/leo"
            android:src="@drawable/leo_selector"
            android:clickable="true"
            android:baselineAlignBottom="false"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:background="@null"/>
        <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/virgo"
            android:src="@drawable/virgo_selector"
            android:clickable="true"
            android:baselineAlignBottom="false"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:background="@null"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:id="@+id/third_line"
        android:layout_below="@+id/second_line"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">
        <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/libra"
            android:src="@drawable/libra_selector"
            android:clickable="true"
            android:baselineAlignBottom="false"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:background="@null"/>
        <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/scorpio"
            android:src="@drawable/scorpio_selector"
            android:clickable="true"
            android:baselineAlignBottom="false"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:background="@null"/>
        <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/sagittarius"
            android:src="@drawable/sagittarius_selector"
            android:clickable="true"
            android:baselineAlignBottom="false"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:background="@null"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:id="@+id/forth_line"
        android:layout_below="@+id/third_line"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">
        <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/capricorn"
            android:src="@drawable/capricorn_selector"
            android:clickable="true"
            android:baselineAlignBottom="false"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:background="@null"/>
        <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/aquarius"
            android:src="@drawable/aquarius_selector"
            android:clickable="true"
            android:baselineAlignBottom="false"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:background="@null"/>
        <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/pisces"
            android:src="@drawable/pisces_selector"
            android:clickable="true"
            android:baselineAlignBottom="false"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:cropToPadding="false"
            android:background="@null"/>
    </LinearLayout>
</RelativeLayout>
